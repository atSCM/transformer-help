{
  "title": "Button",
  "description": "Fires trigger \"clicked\".",
  "parameters": [
    {
      "name": "base",
      "valuetype": "address",
      "desc": "base",
      "behavior": "hidden"
    },
    {
      "name": "label",
      "valuetype": "trstring",
      "substitute": "$LABEL$",
      "defaultvalue": "T{Basic}",
      "desc": "label",
      "behavior": "optional"
    },
    {
      "name": "fontFamily",
      "config": "Times New Roman=Times New Roman,Bodoni,Garamond,Minion Web,ITC Stone Serif,MS Georgia,Bitstream Cyberbit,serif;Arial=MS Trebuchet,ITC Avant Garde Gothic,MS Arial,MS Verdana,Univers,Futura,ITC Stone Sans,Gill Sans,Akzidenz Grotesk,Helvetica,sans-serif;Courier=Courier,MS Courier New,Prestige,Everson Mono,monospace;Zapf-Chancery=Caflisch Script,Adobe Poetica,Sanvito,Ex Ponto,Snell Roundhand,Zapf-Chancery,cursive;Alpha Geometrique=Alpha Geometrique,Critter,Cottonwood,FB Reactor,Studz,fantasy",
      "group": "Font",
      "valuetype": "enum",
      "substitute": "$FONTFAMILY$",
      "defaultvalue": "Arial",
      "desc": "family",
      "behavior": "hidden"
    },
    {
      "name": "fontSize",
      "config": "6;7;8;9;10;11;12;14;16;18;20;22;24;26;28;36;48;72",
      "group": "Font",
      "valuetype": "enum",
      "substitute": "$FONTSIZE$",
      "defaultvalue": "12",
      "desc": "size",
      "behavior": "optional"
    },
    {
      "name": "fontColor",
      "group": "Font",
      "valuetype": "global",
      "substitute": "$FONTCOLOR$",
      "defaultvalue": "SYSTEM.GLOBALS.atvFontColor",
      "desc": "color",
      "behavior": "optional"
    },
    {
      "name": "visibilityLabel",
      "config": "visible=inherit;hidden=hidden",
      "group": "Appearance",
      "valuetype": "enum",
      "substitute": "$VISIBILITYLABEL$",
      "defaultvalue": "inherit",
      "desc": "visibility label",
      "behavior": "optional"
    },
    {
      "name": "visibilitySymbol",
      "config": "visible=inherit;hidden=hidden",
      "group": "Appearance",
      "valuetype": "enum",
      "substitute": "$VISIBILITYSYMBOL$",
      "defaultvalue": "hidden",
      "desc": "visibility symbol",
      "behavior": "optional"
    },
    {
      "name": "symbol",
      "config": "SYSTEM.LIBRARY.ATVISE.OBJECTDISPLAYS.Symbols.20x20",
      "group": "Appearance",
      "valuetype": "display",
      "substitute": "$SYMBOL$",
      "defaultvalue": "SYSTEM.LIBRARY.ATVISE.OBJECTDISPLAYS.Symbols.20x20.check",
      "desc": "symbol",
      "behavior": "optional"
    },
    {
      "name": "symbolColor",
      "group": "Appearance",
      "valuetype": "global",
      "defaultvalue": "SYSTEM.GLOBALS.atvSymbolColor",
      "desc": "symbol color",
      "behavior": "optional"
    },
    {
      "name": "fillColor",
      "group": "Appearance",
      "valuetype": "global",
      "substitute": "$FILLCOLOR$",
      "defaultvalue": "SYSTEM.GLOBALS.atvFillColor",
      "desc": "fill color",
      "behavior": "optional"
    },
    {
      "name": "fillColorInactive",
      "group": "Appearance",
      "valuetype": "color",
      "defaultvalue": "#b7b7b7",
      "desc": "fill color when inactive",
      "behavior": "optional"
    },
    {
      "name": "strokePressed",
      "group": "Appearance",
      "valuetype": "color",
      "defaultvalue": "#FFFFFF",
      "desc": "stroke color pressed",
      "behavior": "optional"
    },
    {
      "name": "focusStrokeColor",
      "group": "Appearance",
      "valuetype": "color",
      "defaultvalue": "#FFFFFF",
      "desc": "focus stroke color",
      "behavior": "optional"
    },
    {
      "name": "borderColor",
      "group": "Appearance",
      "valuetype": "global",
      "substitute": "$BORDERCOLOR$",
      "defaultvalue": "SYSTEM.GLOBALS.atvBorderColor",
      "desc": "border color",
      "behavior": "optional"
    },
    {
      "name": "extraBorderColor",
      "group": "Appearance",
      "valuetype": "color",
      "defaultvalue": "none",
      "desc": "extra border color",
      "behavior": "optional"
    },
    {
      "name": "right",
      "config": "SYSTEM.SECURITY.RIGHTS",
      "group": "Security",
      "valuetype": "address",
      "desc": "necessary right",
      "behavior": "optional"
    },
    {
      "name": "activeNode",
      "group": "Security",
      "valuetype": "address",
      "desc": "activation address",
      "behavior": "optional"
    },
    {
      "name": "activeValue",
      "group": "Security",
      "valuetype": "string",
      "desc": "activation value",
      "behavior": "optional"
    },
    {
      "name": "consistencyGroup",
      "group": "Consistency",
      "valuetype": "string",
      "desc": "group",
      "behavior": "optional"
    },
    {
      "name": "consistencyFunction",
      "config": "write;read",
      "group": "Consistency",
      "valuetype": "enum",
      "defaultvalue": "write",
      "desc": "function",
      "behavior": "optional"
    },
    {
      "name": "tabIndex",
      "group": "Options",
      "valuetype": "number",
      "desc": "tab index",
      "behavior": "optional"
    },
    {
      "name": "tooltip",
      "group": "Options",
      "valuetype": "trstring",
      "desc": "tooltip",
      "behavior": "optional"
    }
  ]
}
